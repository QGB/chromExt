{
  "name": "qgb tabList",
  "description": "get page",
  "version": "0",
  "permissions": [
    "activeTab",
	"tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["page.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "get"
  },
  "manifest_version": 2
}
